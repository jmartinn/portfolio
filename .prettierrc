{
  "$schema": "http://json.schemastore.org/prettierrc",
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "preserve",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "printWidth": 80,
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "parser": "typescript"
}
